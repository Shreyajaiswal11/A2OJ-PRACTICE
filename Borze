#include <iostream>
using namespace std;

int main() {
char arr[200]={0};
for(int i=0; i<200;i++){
cin>>arr[i];
}
for(int i=0; i<200;i++){
if(arr[i]=='.')
cout<<"0";
if(arr[i] == '-'){
    if(arr[i+1]=='.'){
    cout<<"1";
    i++;}
    else if(arr[i+1]== '-'){
    cout<<"2";
    i++;}
}
}
	return 0;
}
