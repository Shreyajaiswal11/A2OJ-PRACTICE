#include <iostream>
using namespace std;

int main() {
	int t;
	cin>>t;
	int arr[3]={0};
	while(t--){
	    int A[3] ={0};
	    for(int i=0;i<3;i++){
	        cin>>A[i];
	        arr[i]+=A[i];
	    }
	}
	if(arr[0]==0 && arr[1]==0 && arr[2]==0)
	    cout<<"YES";
	    else
	    cout<<"NO";
	return 0;
}
